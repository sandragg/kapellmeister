{
  "name": "kapellmeister",
  "version": "2.0.1",
  "description": "Orchestration For Animated Transitions",
  "source": "src/index.ts",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sghall/kapellmeister.git"
  },
  "keywords": [
    "animation",
    "transition"
  ],
  "author": "Steven Hall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sghall/kapellmeister/issues"
  },
  "homepage": "https://github.com/sghall/kapellmeister#readme",
  "scripts": {
    "docs": "cd docs && npm install && npm run start",
    "prebuild": "npm run build:cln",
    "build": "npm run build:lib && npm run build:cpy",
    "build:cln": "rimraf build",
    "build:lib": "node ./scripts/build.js",
    "build:cpy": "cross-env BABEL_ENV=cjs babel-node ./scripts/copy-files.js",
    "test": "cross-env BABEL_ENV=test mocha \"src/**/*.spec.ts\"",
    "test:watch": "npm run test -- -w",
    "lint": "tslint -p tsconfig.json -c tslint.json \"src/**/*.ts\" -e \"*.spec.ts\"",
    "test:coverage": "cross-env nyc mocha \"src/**/*.spec.ts\" && nyc report --reporter=lcov",
    "test:coverage:html": "cross-env BABEL_ENV=coverage nyc mocha \"src/**/*.spec.js\" && nyc report --reporter=html",
    "pretty": "prettier src/**/*.ts --write --no-semi --single-quote --trailing-comma=all --print-width 80",
    "copy": "cp -r build/.  ../react-move/node_modules/kapellmeister/"
  },
  "dependencies": {
    "@types/d3-timer": "^1.0.9",
    "d3-timer": "^1.0.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-transform-computed-properties": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/plugin-transform-typescript": "^7.3.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "@types/chai": "^4.1.7",
    "@types/d3-interpolate": "^1.3.1",
    "@types/mocha": "^5.2.6",
    "@types/sinon": "^7.0.8",
    "babel-plugin-istanbul": "^5.1.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "d3-interpolate": "^1.3.2",
    "fs-extra": "^7.0.1",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "rollup": "^1.4.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-size-snapshot": "^0.8.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-uglify": "^6.0.2",
    "sinon": "^7.2.6",
    "source-map-support": "^0.5.10",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.1",
    "typescript": "^2.9.2",
    "webpack": "^4.29.6"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.spec.ts"
    ],
    "all": true
  }
}
